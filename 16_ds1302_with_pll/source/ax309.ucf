
NET clk LOC = T8 | TNM_NET = sys_clk_pin | IOSTANDARD = "LVCMOS33";
TIMESPEC TS_sys_clk_pin = PERIOD sys_clk_pin 50000kHz;

NET rst_n LOC = L3 | IOSTANDARD = "LVCMOS33";


# RTC
NET rtc_ce  LOC = C13 | IOSTANDARD = "LVCMOS33";
NET rtc_sclk LOC = E13 | IOSTANDARD = "LVCMOS33";
NET rtc_sio  LOC = D14 | IOSTANDARD = "LVCMOS33";

# Tx
NET tx_pin LOC = D12 | IOSTANDARD = "LVCMOS33";
